@model  InstitucionVista
@{
    ViewBag.Title = "Modificar Institucion";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using (Html.BeginForm("Modificar_Institucion", "Instituciones", FormMethod.Post, new { Id = "FormularioInstituciones" }))
{ 
    <div id="titulo" style="width: 900px; height: 30px; padding: 10px 5px 0 5px; font-family: Arial, Helvetica, sans-serif;
        font-size: 16px; font-weight: bold;">
        Modificar Instituto</div>
    <table>
        <tr>
            <td>
                Nombre
            </td>
            <td>
                @Html.TextBoxFor(c => c.N_INSTITUCION, new { style = "width:200px;", MaxLength = "50" })
            </td>
        </tr>
        <tr>
            <td>
                Propia
            </td>
            <td>
                @Html.CheckBoxFor(c => c.INS_PROPIA)
            </td>
        </tr>
    </table>
    @Html.HiddenFor(d => d.ID_INSTITUCION)
    <p>
        <input type="submit" value="Guargar" />
        @Html.ActionLink("Cancelar", "Index", "Instituciones")
    </p>
}
<script type="text/javascript">
    $(document).ready(function () {




    });
</script>
