@model IList<IPersona>

    Usuarios:
    @if (Model != null && Model.Count()>0)
{
        <table id="UsuariosLista" border="0" cellpadding="0" cellspacing="0">
            <tr>
                <th>
                    DNI
                </th>
                <th>
                    APELLIDO
                </th>
                <th>
                    NOMBRE
                </th>
                <th>
                    EMAIL
                </th>
            </tr>
            @foreach (var item in Model)
        {
                <tr>
                    <td>
                        @item.PersonaDni
                    </td>
                    <td>
                        @item.PersonaApellido
                    </td>
                     <td>
                        @item.PersonaNombre
                    </td>
                    <td>
                        @item.PersonaEmail
                    </td>
                   
                </tr>
           
    
        }
        </table>
}

