@model IList<IUsuario>

    Usuarios:
    @if (Model != null && Model.Count()>0)
{
        <table id="UsuariosLista" border="0" cellpadding="0" cellspacing="0">
            <tr>
                <th>
                    Usuario
                </th>
                <th>
                    Password
                </th>
            </tr>
            @foreach (var item in Model)
        {
                <tr>
                    <td>
                        @item.Nombre
                    </td>
                    <td>
                        @item.Password
                    </td>
                </tr>
           
    
        }
        </table>
}

