@model  EstablecimientoVista
@{
    ViewBag.Title = "SGC - Gobierno de Córdoba - Establecimientos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using (Html.BeginForm("Agregar_Establecimiento", "Establecimientos", FormMethod.Post, new { Id = "FormularioEstablecimiento" }))
{ 
    <div id="titulo">
        <div id="ttxt">
            Agregar Establecimiento</div>
    </div>
    <table id="myIntituciones" align="center" width="630px">
        <tr>
            <td>
                Nombre
            </td>
            <td>
                @Html.TextBoxFor(c => c.N_Establecimiento, new { style = "width:200px;", MaxLength = "50" })
            </td>
        </tr>
    </table>
    
    
    @Html.Partial("_Domicilios", Model.ListaDomicilio)
    <table id="mybusqueda" align="center" width="630px">
        <tr>
            <td>
                Provincia
            </td>
            <td>
                <input id="Provincia" disabled="True" style="width: 200px;" />
            </td>
        </tr>
        <tr>
            <td>
                Localidad
            </td>
            <td>
                <input id="Localidad" disabled style="width: 350px;" />
            </td>
        </tr>
        <tr>
            <td>
                Barrio
            </td>
            <td>
                <input id="Barrio" disabled style="width: 350px;" />
            </td>
        </tr>
        <tr>
            <td>
                Calle
            </td>
            <td>
                <input id="Calle" disabled style="width: 350px;" />
            </td>
        </tr>
        <tr>
            <td>
                Nro
            </td>
            <td>
                <input id="Nro" disabled style="width: 60px;" />
            </td>
        </tr>
    </table>
    
    
  
    
    @Html.HiddenFor(d => d.Id_Domicilio)
    <table id="myIntituciones" align="center" width="630px">
        <tr>
            <td>
                <input type="submit" value="Guargar" class="btnIngresar" />&nbsp;&nbsp;
                @Html.ActionLink("Cancelar", "Index", "Establecimientos", null, new { Class = "btnIngresar" })
            </td>
        </tr>
    </table>
   
}
<script type="text/javascript">
    $(document).ready(function () {


    });
</script>
