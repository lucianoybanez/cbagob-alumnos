@model IList<IDocentes>
@if (Model != null && Model.Count > 0)
{ 
    <div id="tab" style="text-align: center;">
         <table align="center" id="mytable" width="950px"  cellpadding="0" cellspacing="0">
            <thead>
                <tr>
                    <th height="30">
                        <div align="center">
                            CUIT</div>
                    </th>
                     <th height="30">
                        <div align="center">
                            RAZON SOCIAL</div>
                    </th>
                    <th height="30">
                        <div align="center">
                            MODALIDAD</div>
                    </th>
                     <th height="30" colspan="7">
                        <div align="center">
                        ACCION
                        </div>
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td align="left">
                           @item.Cuit
                        </td> 
                         <td align="left">
                            @item.Razon_Social
                        </td>
                        <td align="left">
                            @item.N_Modalidad
                        </td> 
                         <td colspan="7">
                            @Html.ActionLink("Modificar", "Modificar", "Docentes", new { id_docente = item.Id_Docente }, null)
                            @Html.ActionLink("Eliminar", "Eliminar", "Docentes", new { id_docente = item.Id_Docente }, null)
                        </td>
                    </tr>
                }
                 <tr>
                        <td align="left" colspan="7">
                            &nbsp;
                        </td>
                        
                    </tr>
                    <tr>
                        <td align="left" valign="middle" colspan="4" height="20">
                            <div id="resumenPag" style="height:30px; padding:10px 5px 0 2px;">
                              <div align="center">
                                <ul id="pagination-clean">
                                  <li class="previous-off">Ant</li>
                                  <li class="active">1</li>
                                  <li><a href="?page=2">2</a></li>
                                  <li><a href="?page=3">3</a></li>
                                  <li><a href="?page=4">4</a></li>
                                  <li><a href="?page=5">5</a></li>
                                  <li><a href="?page=6">6</a></li>
                                  <li><a href="?page=7">7</a></li>
                                  <li class="next"><a href="?page=2">Sig</a></li>
                                </ul>
                              </div>
                         </div>
                        </td> 
                        <td align="right" valign="bottom" colspan="3" height="20">
                         <div id="resumenPag" style="height:30px; padding:15px 5px 0 2px;">
                            @Html.ActionLink("Nuevo", "Agregar", "Establecimientos", null, new { Class = "btngral" })
                         </div>
                        </td> 
                    </tr>
            </tbody>
        </table>
    </div>
           
}
else
{
    <div id="tab" style="text-align: center;">
        <table align="center" id="mytable">
            <thead>
                <tr>
                    <th height="30">
                        <div align="center">
                            SIN DATOS</div>
                    </th>
                </tr>

            </thead>
        </table>
    </div>
}


