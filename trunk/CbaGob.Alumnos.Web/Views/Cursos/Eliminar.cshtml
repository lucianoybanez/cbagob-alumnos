@model CbaGob.Alumnos.Servicio.VistasInterface.ICursosVista
@{
    ViewBag.Title = "SGC - Gobierno de Córdoba - Cursos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Scripts{
    @Content.Script("CbaGob.Alumnos.Cursos.js",Url)
}
@using (Html.BeginForm("Eliminar_Cursos", "Cursos", FormMethod.Post, new { Id = "FormularioCursos" }))
{ 
    <div id="titulo">
        <div id="ttxt">
            Eliminar Curso</div>
    </div>
     <table align="center" id="mytable" style="font-family:Arial, Helvetica, sans-serif; font-size:12px; width:750px;">
        <tr>
            <td>
  <fieldset style="font-family:Arial, Helvetica, sans-serif; font-size:12px; width:750px; text-align:center;">
                <legend>Nuevo Curso</legend>
    <table align="center" id="mytable" style="font-family:Arial, Helvetica, sans-serif; font-size:12px; width:750px;">
        <tr>
            <td>
                Nombre
            </td>
            <td>
                @Html.TextBoxFor(c => c.N_CURSO, new { disabled = "disabled", style = "width:200px;", MaxLength = "50", Class = "asd" })
            </td>
        </tr>
        <tr>
            <td>
                Nro Resolucion
            </td>
            <td>
                @Html.TextBoxFor(c => c.NRORESOLUCION, new { disabled = "disabled", style = "width:200px;", MaxLength = "50", Class = "asd" })
            </td>
        </tr>
   </table>
      </fieldset>
     </td>
        </tr>
    </table>
    @Html.HiddenFor(d => d.ID_CURSO)
    
     <table id="myIntituciones" align="center" width="800px">
        <tr>
            <td align="right" vlign="middle">
