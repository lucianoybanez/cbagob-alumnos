@model IList<ICursos>

@if (Model != null && Model.Count > 0)
{ 
    <div id="tab" style="text-align: center;">
        <table align="center" id="mytable" width="950px"  cellpadding="0" cellspacing="0">
            <thead>
                <tr>
                    <th height="30">
                        <div align="center">
                            NOMBRE</div>
                    </th>
                     <th height="30">
                        <div align="center">
                            NRO RESOLUCION</div>
                    </th>
                     <th height="30">
                        <div align="center">
                        </div>
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td align="left">
                            @item.N_CURSO
                        </td> 
                         <td align="left">
                            @item.NRORESOLUCION
                        </td> 
                         <td>
                            @Html.ActionLink("Modificar", "Modificar", "Cursos", new { id_curso = item.ID_CURSO }, null)
                            @Html.ActionLink("Eliminar", "Eliminar", "Cursos", new { id_curso = item.ID_CURSO }, null)
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
}
else
{
    <div id="tab" style="text-align: center;">
        <table align="center" id="mytable">
            <thead>
                <tr>
                    <th height="30">
                        <div align="center">
                            SIN DATOS</div>
                    </th>
                </tr>

            </thead>
        </table>
    </div>
}
