@model InscripcionPresentismoVista
<div id="PresentismoDialog" title="Agregar Presentismo">
    @using (Html.BeginForm("GuardarPresentismo", "Inscripciones", FormMethod.Get, new { Id = "FormularioPresentismo" }))
    {
        <input type="hidden" id="PresentismoIdInscripcion" name="PresentismoIdInscripcion"/>
        <table align="left" style="font-family: Arial, Helvetica, sans-serif; font-size: 12px;
            width: 200px;">
            <tr>
                <td align="left">
                    <b>Clases Asistidas:</b>
                </td>
                <td align="left" valign="middle">
                    @Html.TextBoxFor(c => c.ClasesAsistidas)
                </td>
            </tr>
            <tr>
                <td align="left">
                    <b>Presentismo:</b>
                </td>
                <td align="left" valign="middle">
                    @Html.TextBoxFor(c => c.PorcentajePresentismo)
                </td>
            </tr>          
        </table>
        <br />
       
    }
</div>
